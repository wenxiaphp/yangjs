{
  "name": "koa-getting-started",
  "version": "1.0.0",
  "private": true,
  "description": "A sample Node.js app using Koa",
  "main": "server.js",
  "scripts": {
    "up": "lean up --port 3300",
    "fix": "eslint --fix ./",
    "lint": "eslint lint ./",
    "start": "node server.js && yarn build-client",
    "dev": "nodemon server.js -- ",
    "build-client": "cd ./client && yarn install"
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "leancloud-storage": "^3.0.2",
    "leanengine": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": "8.x"
  }
}